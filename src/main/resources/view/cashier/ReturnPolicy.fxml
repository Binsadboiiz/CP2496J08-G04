<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.cashier.ReturnPolicyController">
    <TableView fx:id="policyTable" layoutX="20" layoutY="20" prefWidth="800" prefHeight="400">
        <columns>
            <TableColumn fx:id="colPolicyName" text="Policy Name" prefWidth="200"/>
            <TableColumn fx:id="colDescription" text="Description" prefWidth="400"/>
            <TableColumn fx:id="colDaysAllowed" text="Days Allowed" prefWidth="100"/>
        </columns>
    </TableView>
</AnchorPane>

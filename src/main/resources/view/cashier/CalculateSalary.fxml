<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="controller.cashier.CalculateSalaryController">
    <VBox spacing="10" padding="Insets(20)">
        <Label text="Tính Lương Nhân viên" />
        <Button text="Tính Lương" onAction="#calculateSalary" />
        <Button text="Xem Lịch sử Lương" onAction="#viewSalaryHistory" />
    </VBox>
</AnchorPane>

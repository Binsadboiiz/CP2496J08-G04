<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.cashier.RevenueReportsController">
    <TableView fx:id="reportTable" layoutX="20" layoutY="20" prefWidth="800" prefHeight="400">
        <columns>
            <TableColumn fx:id="colReportType" text="Type" prefWidth="100"/>
            <TableColumn fx:id="colReportDate" text="Date" prefWidth="100"/>
            <TableColumn fx:id="colTotalRevenue" text="Total Revenue" prefWidth="150"/>
            <TableColumn fx:id="colTotalInvoices" text="Invoices" prefWidth="100"/>
            <TableColumn fx:id="colTopSellingProduct" text="Top Product" prefWidth="200"/>
        </columns>
    </TableView>
</AnchorPane>
